<!DOCTYPE html>
<html>
<head>
  <title>Labelwriter</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% if flash[:error] || flash[:notice] %>
    <div><%=flash[:error] || flash[:notice]%></div>
  <% end %>
<%= render 'layouts/layout' %>
<%= yield %>

</body>
</html>
